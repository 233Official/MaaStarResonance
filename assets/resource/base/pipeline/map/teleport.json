{
    "传送地图传送点": {
        "action": {
            "type": "Custom",
            "param": {
                "custom_action": "TeleportPoint"
            }
        }
    },
    "获取参数-传送所需地图": {
        "action": {
            "type": "DoNothing"
        },
        "attach": {
            "dest_map": ""
        }
    },
    "获取参数-传送所需传送点": {
        "action": {
            "type": "DoNothing"
        },
        "attach": {
            "dest_tele_point": ""
        }
    },
    "图片识别是否已经打开地图": {
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "template": ["地图/地图列表图标.png"],
                "roi": [25, 643, 43, 42]
            }
        }
    },
    "图片识别传送点是否可以直接传送": {
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "template": ["地图/地图传送点传送按钮.png"],
                "roi": [1046, 641, 73, 32]
            }
        }
    }
}
