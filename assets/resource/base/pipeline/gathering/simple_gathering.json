{
    "简易采集": {
        "next": [
            "专注采集",
            "普通采集",
            "[JumpBack]从省电模式唤醒"
        ],
        "timeout": 3000, // 3s内如果没识别到采集选项就算超时
        "on_error": ["无物可采"]
    },
    "无物可采": {
        "post_delay": 25000, // 作物重生需要大概 25s
        "next": ["简易采集"]
    },
    "专注采集": {
        "enabled": true,
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "template": ["采集/专注采集字样.png"],
                "roi": [822, 211, 250, 347]
            }
        },
        "action": "Click",
        "post_delay": 5000, // 5s内基本能采集完
        "next": ["简易采集"]
    },
    "普通采集": {
        "enabled": true,
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "template": ["采集/普通采集的左括号采字样.png"],
                "roi": [822, 211, 250, 347]
            }
        },
        "action": {
            "type": "Click",
            "param": {
                "target": true,
                "target_offset": [-10, 0, 0, 0]
            }
        },
        "post_delay": 5000, // 5s内基本能采集完
        "next": ["简易采集"]
    }
}
