{
    "简易采集": {
        "next": [
            "专注采集",
            "普通采集",
            "无物可采"
        ],
        "timeout": 86400000,
        "interrupt": ["从省电模式唤醒"]
    },
    "无物可采": {
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "template": ["采集/采字样.png"],
                "roi": [822, 211, 250, 347]
            }
        },
        "inverse": true, // 无法识别到采集字样则判定为无物可采
        "next": [
            "无物可采-省电模式",
            "确实无物可采"
        ]
    },
    "无物可采-省电模式": {
        "recognition": {
            "type": "OCR",
            "param": {
                "roi": [528, 629, 223, 35],
                "expected": ["省电模式"]
            }
        },
        "action": {
            "type": "Click",
            "param": {
                "target": [591, 4, 33, 17]
            }
        },
        "next": ["无物可采"]
    },
    "确实无物可采": {
        "post_delay": 25000, // 作物重生大概要25s
        "next": ["简易采集"]
    },
    "专注采集": {
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "template": ["采集/专注采集字样.png"],
                "roi": [822, 211, 250, 347]
            }
        },
        "action": "Click",
        "post_delay": 5000, // 5s内基本能采集完
        "next": ["简易采集"]
    },
    "普通采集": {
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "template": ["采集/普通采集的左括号采字样.png"],
                "roi": [822, 211, 250, 347]
            }
        },
        "action": {
            "type": "Click",
            "param": {
                "target": true,
                "target_offset": [-10, 0, 0, 0]
            }
        },
        "post_delay": 5000, // 5s内基本能采集完
        "next": ["简易采集"]
    }
}
