            "adb": {
                // 输入方式：EmulatorExtras > Maatouch > MinitouchAndAdbKey > AdbShell => 按位或 1 | 2 | 4 | 8 为：15
                "input": 15,
                // 截图方式：EmulatorExtras > RawWithGzip > Encode > EncodeToFileAndPull => 按位或 1 | 2 | 4 | 64 为：71
                "screencap": 71
            }
        }
