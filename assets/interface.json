        "采集方案": {
            "cases": [
                {
                    "name": "专注采集",
                    "pipeline_override": {
                        "专注采集": {
                            "enabled": true
                        },
                        "普通采集": {
                            "enabled": false
                        }
                    }
                },
                {
                    "name": "普通采集",
                    "pipeline_override": {
                        "专注采集": {
                            "enabled": false
                        },
                        "普通采集": {
                            "enabled": true
                        }
                    }
                }
            ]
        }
    },
    "advanced": {
        "需要的最大成功钓鱼数量": {
            "field": [
                "需要的最大成功钓鱼数量"
            ],
            "type": [
                "int"
            ],
            "default": [
                "0"
            ],
            "pipeline_override": {
                "启动自动钓鱼入口": {
                    "action": {
                        "type": "Custom",
                        "param": {
                            "custom_action": "AutoFishing",
                            "custom_action_param": {
                                "max_success_fishing_count": "{需要的最大成功钓鱼数量}"
                            }
                        }
                    }
                }
            },
            "doc": "需要的最大成功钓鱼数量，默认设置0为无限钓鱼"
        }
